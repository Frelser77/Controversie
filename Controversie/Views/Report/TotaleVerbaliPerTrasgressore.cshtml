@model IEnumerable<Controversie.Models.TrasgressoreReport>

@{
    ViewBag.Title = "TotaleVerbaliPerTrasgressore";
}

<h2>Totale verbali per trasgressore</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cognome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Totale)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cognome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Totale)
        </td>
    </tr>
}

</table>

<a class="nav-link" href="@Url.Action("Index", "Report")">Indietro</a>